{
  "compilerOptions": {
    "incremental": true,
    "target": "es2017",
    "experimentalDecorators": true,
    "module": "commonjs",
    "outDir": "dist",
    "baseUrl": "src",
    "sourceMap": false,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "strictNullChecks": false,
    "skipLibCheck": true,
    "paths": {
      "#seedwork/*": ["./@seedwork/*"],
      "#category/*": ["./category/*"]
    }
  },
  "include": ["src"],
  "exclude": []
}
